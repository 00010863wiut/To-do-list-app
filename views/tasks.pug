extends layout 

block content 
    .containerAllTasks
        h1 All tasks 

        each task in tasks 
            .task_wrapper
                .taskTitle
                    input.checkbox(type="checkbox" value="1" name="checkbox")
                    a.task(href='#')= task
                a.delete(href='#') Delete
    .circle 
    .circleSecond
    .circleThird
